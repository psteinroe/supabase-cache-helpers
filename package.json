{
  "name": "supabase-cache-helpers",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://psteinroe.github.io/supabase-cache-helpers/",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ci:version": "changeset version && pnpm oxlint --fix && pnpm oxfmt .",
    "ci:version:next": "changeset version --snapshot next && pnpm oxlint --fix && pnpm oxfmt ."
  },
  "devDependencies": {
    "@changesets/cli": "catalog:",
    "oxfmt": "catalog:",
    "oxlint": "catalog:",
    "supabase": "catalog:",
    "turbo": "catalog:"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "9"
  },
  "packageManager": "pnpm@9.15.9",
  "pnpm": {
    "overrides": {
      "react": "catalog:",
      "react-dom": "catalog:"
    }
  }
}
