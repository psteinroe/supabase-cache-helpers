{
  "name": "@supabase-cache-helpers/postgrest-core",
  "version": "0.12.3",
  "keywords": [
    "Cache",
    "PostgREST",
    "Supabase"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psteinroe/supabase-cache-helpers.git",
    "directory": "packages/postgrest-shared"
  },
  "source": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf coverage && rm -rf .nyc_output && rm -rf node_modules && rm -rf dist",
    "lint": "oxlint",
    "test": "vitest --coverage",
    "typecheck": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "fast-equals": "catalog:",
    "flat": "catalog:",
    "merge-anything": "catalog:",
    "xregexp": "catalog:"
  },
  "devDependencies": {
    "@supabase-cache-helpers/oxlint-config": "workspace:*",
    "@supabase-cache-helpers/typescript": "workspace:*",
    "@supabase/postgrest-js": "catalog:",
    "@supabase/supabase-js": "catalog:",
    "@types/flat": "catalog:",
    "@types/lodash": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "dotenv": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@supabase/postgrest-js": "^1.19.4 || ^2.0.0"
  }
}
