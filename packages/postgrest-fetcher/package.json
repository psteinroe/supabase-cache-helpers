  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psteinroe/supabase-cache-helpers.git",
    "directory": "packages/postgrest-fetcher"
  },
  "keywords": [
    "Supabase",
    "PostgREST"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
